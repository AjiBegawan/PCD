package Tampilan;

import java.awt.image.BufferedImage;
import java.io.File;

import javax.imageio.ImageIO;
import javax.imageio.stream.ImageInputStream;

public class GrayScale {
	public static void toGrayScale (File alamatInput) {
		try {
			ImageInputStream gambar = ImageIO.createImageInputStream(alamatInput);
			BufferedImage image = ImageIO.read(gambar);
		}
		catch{
			
		}
	}
}
